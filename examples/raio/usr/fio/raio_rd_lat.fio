; Read 4 files with aio at different depths
[global]
group_reporting
thread
ioengine=./.libs/libraio_fio.so
rw=randread
bs=1K
loops=1
direct=1
iodepth=1
filename=/192.168.1.66/1234/dev/null
;filename=/192.168.1.67/1234/dev/ram2

[file1]
cpus_allowed=1

;[file2]
;cpus_allowed=2

;[file3]
;cpus_allowed=3

;[file4]
;cpus_allowed=4

;[file5]
;cpus_allowed=5
